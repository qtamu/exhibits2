<% manifest = document.manifest %>

<% if manifest %>
  <iframe
    src="<%= mirador_index_path(
      manifest: manifest
    ) %>"
    allowfullscreen="true"
    style="height: 70vh"
    frameborder="0"
    marginwidth="0"
    marginheight="0"
    scrolling="no"
    width="100%"
  ></iframe>
<% end %>
